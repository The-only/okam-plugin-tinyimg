{
  "name": "example",
  "version": "1.0.0",
  "description": "example",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "fecs ./src --reporter=baidu --rule --type=vue,js",
    "precommit": "lint-staged",
    "init": "npm i",
    "build": "npm run clean && NODE_ENV=dev node scripts/build.js",
    "prod": "npm run clean && node scripts/build.js",
    "dev:debug": "NODE_ENV=dev node --inspect-brk scripts/build.js --watch",
    "dev": "NODE_ENV=dev node scripts/build.js --watch"
  },
  "lint-staged": {
    "linters": {
      "*.{js,vue}": [
        "fecs --reporter=baidu --rule --type=vue,js"
      ]
    },
    "ignore": [
      "dist/**/*"
    ]
  },
  "keywords": [
    "example"
  ],
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "chalk": "^2.4.1",
    "fecs": "~1.4.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "postcss": "^7.0.2",
    "rider": "^2.0.0",
    "rimraf": "^2.6.2",
    "stylus": "^0.54.5",
    "yargs-parser": "^10.1.0"
  },
  "engine": {
    "node": ">= 6"
  },
  "dependencies": {}
}
